						<td width="20%"
							class="indent2">Método</td>
						<td width="80%">GET</td>
					</tr>
					<tr>
						<td width="20%"
							class="indent2">Parámetros</td>
						<td width="80%"></td>
					</tr>
					<tr>
						<td width="20%"
							class="indent2">Ataque</td>
						<td width="80%"></td>
					</tr>
					<tr>
						<td width="20%"
							class="indent2">Evidencia</td>
						<td width="80%">Golfe2</td>
					</tr>
